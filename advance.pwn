				f("За этот час было куплено %d билетов счастливое число этого часа: {ff9a00}%d%d%d",lotoall,lotoone,lototwo,lototri);
				SCM(i,0x63cb00ff,string);
				if(GetPVarInt(i, "LOTO1") == lotoone)
				    totalNums++;
				if(GetPVarInt(i, "LOTO2") == lototwo)
				    totalNums++;
				if(GetPVarInt(i, "LOTO3") == lototri)
				    totalNums++;
				switch(totalNums)
				{
				    case 0:
				    {
						SCM(i, 0xFFFFFF, "К сожалению в вашем билете не совпала первая цифра");
						SCM(i, -1, "Не расстраивайтесь. В следующий раз Вам обязательно повезет");
				    }
				    case 1:
				    {
						SCM(i,0x5AB200FF, "В вашем билете совпало 1 цифра. Поздравляем, ваш выигрыш - 2.000$");
						Player[i][pMoney] += 2000;
				    }
				    case 2:
				    {
						SCM(i,0x5AB200FF, "В вашем билете совпали 2 цифры. Поздравляем, ваш выигрыш - 15.000$");
						Player[i][pMoney] += 15000;
				    }
				    case 3:
				    {
						SCM(i, 0x5AB200FF, "В вашем билете совпали все цифры. Поздравляем, ваш выигрыш - 50.000$");
						Player[i][pMoney] += 50000;
				    }
				}
				Player[i][pLoto] = false;
				DeletePVar(i, "LOTO1");
				DeletePVar(i, "LOTO2");
				DeletePVar(i, "LOTO3");
			}
		}
	}
	for(new h = 0; h < TotalHouse; h++)
	{
