				if(params[1] == 1)
				{
					if(Player[params[0]][pLicDrive] == 1) return SCM(playerid, COLOR_GRAD, "У гражданина уже есть данный вид лицензии");
				}
				if(params[1] == 2)
				{
					if(Player[params[0]][pLicDrive] == 2) return SCM(playerid, COLOR_GRAD, "У гражданина уже есть данный вид лицензии");
				}
				if(params[1] == 3)
				{
					if(Player[params[0]][pLicGun] == 1) return SCM(playerid, COLOR_GRAD, "У гражданина уже есть данный вид лицензии");
				}
				if(GetPVarInt(playerid,"yes") == 1) return SCM(playerid,COLOR_GRAD, "Вы не можете использовать это в данный момент, отзовите своё предыдущее предложение {0099FF}/cancel");
				SetPVarInt(params[0],"buylic",1);
				SetPVarInt(playerid,"yes",1);
