                        format(query, sizeof(query), "При регистрации Вы указали ник пригласившего Вас игрока %s.", Player[i][pAward]);
                        SCM(i, 0xFFCC00AA, query);
                        SCM(i, 0x0099FFAA, "Этому игроку выдано вознаграждение в размере {00cc00}50.000$");
                    }
                    else
                    {
                        // Обновление денег в базе данных для игрока, если он не в игре
                        new upfive[80];
                        format(upfive, sizeof(upfive), "SELECT * FROM accounts WHERE name = '%s'", Player[i][pAward]);
                        new Cache: ramil_jopa = mysql_query(connect_mysql, upfive, true);

                        if(cache_num_rows() > 0)
                        {
                            new getnamers[100];
                            new setplus, rows, fields, temp[40];
                            format(getnamers, sizeof(getnamers), "SELECT money FROM `accounts` WHERE name = '%s' LIMIT 1", Player[i][pAward]);
                            new Cache: lolpizda = mysql_query(connect_mysql, getnamers, true);
                            cache_get_data(rows, fields, connect_mysql);

                            if(rows)
                            {
                                cache_get_field_content(0, "Money", temp);
                                setplus = strval(temp);
                            }

                            cache_delete(lolpizda);
                            setplus += 50000;

                            new setasers[90];
                            format(setasers, sizeof(setasers), "UPDATE accounts SET money = '%i' WHERE name = '%s'", setplus, Player[i][pAward]);
                            mysql_query(connect_mysql, setasers, false);
                        }

                        cache_delete(ramil_jopa);
                    }
                }
            }
        }
    }
    return 1;
}
