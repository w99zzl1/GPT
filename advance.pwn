public TaxiArrived(playerid)
{
    SCM(playerid, 0xFFCC00FF, "AI Такси: {FFFFFF}Мы прибыли!");
    RemovePlayerFromVehicle(playerid);
    SetTimer("Taxi_Reset", 2000, false);
    
    if (Player[playerid][pQuests] >= 1) return;
    
    PlayerPlaySound(playerid, 1185, 0.0, 0.0, 0.0);
    Player[playerid][pMoney] += 1000;
    Player[playerid][pExp] += 2;
    Player[playerid][pQuests] = 1;
    
    SPD(playerid, 100, DIALOG_STYLE_MSGBOX, "Такси", "Квест завершен! Награда: 5000 виртов, 2 EXP.", "Ок", "");
    
    new query[128], name[MAX_PLAYER_NAME];
    GetPlayerName(playerid, name, sizeof(name));
	mysql_format(connect_mysql, query, sizeof(query), "UPDATE `accounts` SET `quests` = '1' WHERE `IDacc` = '%d'", Player[playerid][pIDacc]);
	mysql_query(connect_mysql, query, true);
}

