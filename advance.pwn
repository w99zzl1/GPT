CMD:drugs(playerid, params[])
{
    if(Player[playerid][pMember] >= 6 && Player[playerid][pMember] <= 9 || Player[playerid][pMember] == 0 || Player[playerid][pMember] == 13)
    {
		if(GetPVarInt(playerid,"DrugsEffect") == 1) return SCM(playerid, COLOR_GRAD, "Нельзя употреблять наркотики слишком часто...");
		if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_GRAD, "Используйте: /drugs [грамм]");
		if(params[0] <= 0) return SCM(playerid, COLOR_GRAD, "Неверное кол-во грамм");
		if(params[0] > 10) return SCM(playerid, COLOR_GRAD, "Не более 10 грамм за раз");
		if(Player[playerid][pDrugs] < params[0]) return SCM(playerid,COLOR_GRAD, "У Вас нет столько грамм наркотиков");
		if(params[0] >= 1 && params[0] <= 3)
		{
			if(Player[playerid][pHealth] < 100.0) Player[playerid][pHealth] += 5.0;
			if(Player[playerid][pHealth] > 100.0) Player[playerid][pHealth] = 100.0;
			SetTimerEx("DrugsEffect", 6000, false, "i", playerid);
			SetPlayerDrunkLevel(playerid, 3000);
		}
		if(params[0] >= 4 && params[0] <= 6)
		{
			if(Player[playerid][pHealth] < 100.0) Player[playerid][pHealth] += 20.0;
			if(Player[playerid][pHealth] > 100.0) Player[playerid][pHealth] = 100.0;
			SetTimerEx("DrugsEffect", 12000, false, "i", playerid);
			SetPlayerDrunkLevel(playerid, 6000);
		}
		if(params[0] >= 7 && params[0] <= 10)
		{
			if(Player[playerid][pHealth] < 100.0) Player[playerid][pHealth] += 35.0;
			if(Player[playerid][pHealth] > 100.0) Player[playerid][pHealth] = 100.0;
			SetTimerEx("DrugsEffect", 20000, false, "i", playerid);
			SetPlayerDrunkLevel(playerid, 9000);
		}
		if(params[0] >= 11 && params[0] <= 15)
		{
			if(Player[playerid][pHealth] < 100.0) Player[playerid][pHealth] += 50.0;
			if(Player[playerid][pHealth] > 100.0) Player[playerid][pHealth] = 100.0;
			SetTimerEx("DrugsEffect", 30000, false, "i", playerid);
			SetPlayerDrunkLevel(playerid, 15000);
		}
		if(params[0] >= 16 && params[0] <= 20)
		{
			if(Player[playerid][pHealth] < 100.0) Player[playerid][pHealth] += 70.0;
			if(Player[playerid][pHealth] > 100.0) Player[playerid][pHealth] = 100.0;
			SetTimerEx("DrugsEffect", 50000, false, "i", playerid);
			SetPlayerDrunkLevel(playerid, 30000);
		}
		Player[playerid][pDrugs] -= params[0];
		new string[128];
		if(Player[playerid][pSex] == 1) f("{DD90FF}%s употребил(а) {FF9915}%d г {DD90FF}наркотиков", Player[playerid][pName], params[0]);
		if(Player[playerid][pSex] == 2) f("{DD90FF}%s употребил(а) {FF9915}%d г {DD90FF}наркотиков", Player[playerid][pName], params[0]);
		StatusMessage(playerid, string, string);
		f("~r~-%i params[0]", params[0]);
		SetPlayerHealth(playerid, Player[playerid][pHealth]);
		SetPlayerWeather(playerid, -68);
		GameTextForPlayer(playerid, string, 2000, 1);
		SetPlayerSpecialAction(playerid, SPECIAL_ACTION_SMOKE_CIGGY);
		SetPVarInt(playerid,"DrugsEffect",1);
	}
	return 1;
}
