f_public F_OnPlayerConnect(playerid)
{
	SetTimerEx("Update", 5, 1, "i", playerid);
	SetTimerEx("Leker", 5, 1, "i", playerid);
 	return 1;
}
f_public Leker(playerid)
{
	if(GetPlayerAmmo(playerid) != loszer[playerid])
	{
		loszer[playerid] = GetPlayerAmmo(playerid);
	}
}
f_public Update(playerid)
{
	{
		if(loszer[playerid] == GetPlayerAmmo(playerid))
		{
			Lott[playerid] = false;
		}
		if(loszer[playerid] > GetPlayerAmmo(playerid) && weap[playerid] == GetPlayerWeapon(playerid))
		{
			loszer[playerid] = GetPlayerAmmo(playerid);
			Lott[playerid] = true;
			if(Lott[playerid] == true)
			{
				CallLocalFunction("OnPlayerShoot", "iii", playerid, weap[playerid], loszer[playerid]);
			}
		}
	}
}
f_public OnPlayerShoot( playerid, weaponid, ammo )
{
	if(GetPlayerWeapon(playerid) == 24) Player[playerid][pOldAmmo] = GetPlayerAmmo(playerid);
	if(GetPlayerWeapon(playerid) == 25) Player[playerid][pOldAmmo] = GetPlayerAmmo(playerid);
	if(GetPlayerWeapon(playerid) == 29) Player[playerid][pOldAmmo] = GetPlayerAmmo(playerid);
	if(GetPlayerWeapon(playerid) == 30) Player[playerid][pOldAmmo] = GetPlayerAmmo(playerid);
	if(GetPlayerWeapon(playerid) == 31) Player[playerid][pOldAmmo] = GetPlayerAmmo(playerid);
	if(GetPlayerWeapon(playerid) == 34) Player[playerid][pOldAmmo] = GetPlayerAmmo(playerid);
	Player[playerid][pNewAmmo] -= ammo -= Player[playerid][pOldAmmo];
	if(GetPlayerWeapon(playerid) == 24) weapon_anti_cheat[playerid][2] += Player[playerid][pNewAmmo]-1;
	if(GetPlayerWeapon(playerid) == 24 && weapon_anti_cheat[playerid][2] <= 0) weapon_anti_cheat[playerid][2] = 0;
	if(GetPlayerWeapon(playerid) == 25) weapon_anti_cheat[playerid][3] += Player[playerid][pNewAmmo]-1;
	if(GetPlayerWeapon(playerid) == 25 && weapon_anti_cheat[playerid][3] <= 0) weapon_anti_cheat[playerid][3] = 0;
	if(GetPlayerWeapon(playerid) == 29) weapon_anti_cheat[playerid][4] += Player[playerid][pNewAmmo]-1;
	if(GetPlayerWeapon(playerid) == 29 && weapon_anti_cheat[playerid][4] <= 0) weapon_anti_cheat[playerid][4] = 0;
	if(GetPlayerWeapon(playerid) == 30) weapon_anti_cheat[playerid][5] += Player[playerid][pNewAmmo]-1;
	if(GetPlayerWeapon(playerid) == 30 && weapon_anti_cheat[playerid][5] <= 0) weapon_anti_cheat[playerid][5] = 0;
	if(GetPlayerWeapon(playerid) == 31) weapon_anti_cheat[playerid][5] += Player[playerid][pNewAmmo]-1;
	if(GetPlayerWeapon(playerid) == 31 && weapon_anti_cheat[playerid][5] <= 0) weapon_anti_cheat[playerid][5] = 0;
	if(GetPlayerWeapon(playerid) == 34) weapon_anti_cheat[playerid][6] += Player[playerid][pNewAmmo]-1;
	if(GetPlayerWeapon(playerid) == 34 && weapon_anti_cheat[playerid][6] <= 0) weapon_anti_cheat[playerid][6] = 0;
 	return 1;
}
f_public DiceOff()
{
    foreach(Player, i)
	{
        if(hands_down[i] == 1) hands_down[i] = 0;
    	if(IsPlayerAttachedObjectSlotUsed(i,1)) RemovePlayerAttachedObject(i,1);
    	ClearAnimations(i);
	}
}
