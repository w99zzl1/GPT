	SCMTA(COLOR_WHITE, "    БАНКОВСКИЙ ЧЕК");
	SCMTA(COLOR_WHITE, "______________________");
	new lotoone = random(9);
	new lototwo = random(9);
	new lototri = random(9);
	foreach(new i : Player)
	{
		if(login_check[i] == true && Player[i][pInGameTime] < 20)
		{
		    if(Player[i][pAFK] == false)
		    {
				SCM(i,0xFFCC00AA, "Для получения зарплаты Вы должны находиться в игре 20 минут.");
				Player[i][pInGameTime] = 0;
