	        mysql_query(connect_mysql, "UPDATE `houses` SET `evict` = `evict` - '1' WHERE `evict` > '1'");
	    }
	}
	foreach(new a: Player)
	{
		if(IsPlayerConnected(a))
		{
			if(login_check[a] == true)
			{
				SaveAll(a);
			}
		}
	}
	lotoall = 0;
}
