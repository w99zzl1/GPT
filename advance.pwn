			if(GZInfo[i][gMemberZone] != Player[playerid][pMember]) return SCM(playerid,COLOR_GRAD, "Вы должны находиться на территории своей банды.");
			if(!IsPlayerConnected(params[0])) return SCM(playerid,COLOR_GRAD,"Такого игрока нет");
			if(GetPlayerWeapon(playerid) == 0) return SCM(playerid,COLOR_GRAD, "У Вас нет оружия");
