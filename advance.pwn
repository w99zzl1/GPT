CMD:sp(playerid, params[])
{
    if(Player[playerid][pAdmin] < 1) return 1;
    {
	    if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_GRAD, !"Используйте: /sp [id игрока]");
		if(IsPlayerConnected(params[0]))
		{
			if(params[0] != INVALID_PLAYER_ID)
			{
				if(login_check[params[0]] == false) return SCM(playerid, COLOR_GRAD, !"Игрока с таким id нет");
				if(params[0] == playerid) return SCM(playerid, COLOR_GRAD, !"Нельзя наблюдать за самим собой");
				if(sp[params[0]] == true) return SCM(playerid, COLOR_GRAD, !"[Ошибка] SPECPLAYER [#1]");
				if(sp[playerid] == false)
				{
				    Player[playerid][pSpectrec] = true;
				    new Float: posx, Float: posy, Float: posz;
					GetPlayerFacingAngle(playerid, AngleSP[playerid]);
					GetPlayerPos(playerid, posx, posy, posz);
					GetPlayerPos(playerid, anti_cheat[playerid][XXXX], anti_cheat[playerid][YYYY], anti_cheat[playerid][ZZZZ]);
					SetPVarFloat(playerid, "UnSpecPosX", posx);
					SetPVarFloat(playerid, "UnSpecPosY", posy);
					SetPVarFloat(playerid, "UnSpecPosZ", posz);
					SetPVarInt(playerid, "UnSpecVirtualWorld", GetPlayerVirtualWorld(playerid));
					SetPVarInt(playerid, "UnSpecInterior", GetPlayerInterior(playerid));
					anti_cheat[playerid][pOpen] = true;
					if(GetPlayerWeapon(playerid) > 0)
					{
					    SetPVarInt(playerid, "UnSpecWeapon", GetPlayerWeapon(playerid));
					    SetPVarInt(playerid, "UnSpecAmmo", GetPlayerAmmo(playerid));
					}
					DeletePVar(playerid, "UnSpecWeapon");
					DeletePVar(playerid, "UnSpecAmmo");
				}
				anti_cheat[playerid][pTimerAC] = 9999;
				if(GetPlayerState(params[0]) == 1)
				{
					HideMenuForPlayer(spmenu, playerid);
					ShowMenuForPlayer(spmenu, playerid);
					TogglePlayerSpectating(playerid, 1);
					PlayerSpectatePlayer(playerid, params[0]);
					SetPlayerInterior(playerid,GetPlayerInterior(params[0]));
					SetPlayerVirtualWorld(playerid,GetPlayerVirtualWorld(params[0]));
					SpecPlayerID[playerid] = params[0];
					gSpectateID[playerid] = params[0];
					gSpectateType[playerid] = 1;
					sp[playerid] = true;
				}
				else if(GetPlayerState(params[0]) == 2)
				{
					new idcara;
					HideMenuForPlayer(spmenu, playerid);
					ShowMenuForPlayer(spmenu, playerid);
					TogglePlayerSpectating(playerid, 1);
					idcara = GetPlayerVehicleID(params[0]);
					PlayerSpectateVehicle(playerid, idcara);
					SetPlayerInterior(playerid,GetPlayerInterior(params[0]));
					SetPlayerVirtualWorld(playerid,GetPlayerVirtualWorld(params[0]));
					SpecPlayerID[playerid] = params[0];
					gSpectateID[playerid] = params[0];
					gSpectateType[playerid] = 2;
					sp[playerid] = true;
				}
				else if(GetPlayerState(params[0]) == 3)
				{
					new idcara;
					HideMenuForPlayer(spmenu, playerid);
					ShowMenuForPlayer(spmenu, playerid);
					TogglePlayerSpectating(playerid, 1);
					idcara = GetPlayerVehicleID(params[0]);
					PlayerSpectateVehicle(playerid, idcara);
					SetPlayerInterior(playerid,GetPlayerInterior(params[0]));
					SetPlayerVirtualWorld(playerid,GetPlayerVirtualWorld(params[0]));
					SpecPlayerID[playerid] = params[0];
					gSpectateID[playerid] = params[0];
					gSpectateType[playerid] = 2;
					sp[playerid] = true;
				}
				return 1;
			}
		}
	}
	return 1;
}
CMD:admdown(playerid, params[])
