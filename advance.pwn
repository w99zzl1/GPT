CMD:pay(playerid, params[])
{
    if(sscanf(params, "dd", params[0], params[1])) return SCM(playerid, COLOR_GRAD, !"Используйте: /pay [id игрока] [сумма]");
	if(params[1] < 1 || params[1] > 5000) return SCM(playerid, COLOR_GRAD, !"Не меньше 1$ и не более 5000$");
	if(Player[playerid][pMoney] < params[1]) return SCM(playerid, COLOR_GRAD, !"У Вас нет столько денег.");
	if(params[0] == playerid) return SCM(playerid, COLOR_GRAD, !"Вы указали свой id");
	if(login_check[params[0]] == false) return SCM(playerid, COLOR_GRAD, !"Игрока с таким id нет");
	if(GetDistanceBetweenPlayers(playerid, params[0]) > 3 || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0])) return SCM(playerid, COLOR_GRAD, !"Игрок слишком далеко от Вас");
	Player[playerid][pMoney] -= params[1];
	Player[params[0]][pMoney] += params[1];
	Player[playerid][pPay] = params[0];
	ApplyAnimation(playerid, "DEALER", "shop_pay", 4.0999, 0, 1, 1, 1, 1, 4670);
	format(query, sizeof(query), "Вы передали игроку %s %i$", Player[params[0]][pName],params[1]);
	SCM(playerid, 0x3295f8AA, query);
	format(query, sizeof(query), "%s передал(а) Вам %i$",Player[playerid][pName],params[1]);
	SCM(params[0], 0x3295f8AA, query);
	format(query, sizeof(query), "-%d$",params[1]);
	SetPlayerChatBubble(playerid, query, 0xFF6600AA, 15.0, 3000);
	format(query, sizeof(query), "+%d$",params[1]);
	SetPlayerChatBubble(params[0], query, COLOR_GREEN, 15.0, 3000);
	if(Player[playerid][pSex] == 1) format(query, sizeof(query), "%s достал кошелек и передал деньги",Player[playerid][pName]);
	if(Player[playerid][pSex] == 2) format(query, sizeof(query), "%s достала кошелек и передала деньги",Player[playerid][pName]);
	StatusMessage(playerid, query, "достал(а) кошелек и передал(а) деньги");
	PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
	SaveAll(playerid);
	SaveAll(params[0]);
	return 1;
}
