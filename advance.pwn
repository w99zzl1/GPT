public OnPlayerText(playerid, text[])
{
    new SAMPFilesru = GetTickCount();
	if(GetPVarInt(playerid, "AntiFloodText") > SAMPFilesru)
	{
		Kick(playerid);
	}
	else SetPVarInt(playerid, "AntiFloodText", SAMPFilesru+50);
	if(chat == true)
	{
		if(anti_cheat[playerid][pFlood] == true)
		{
			SCM(playerid,COLOR_GRAD, "Не флудите");
			return 0;
		}
		if(Player[playerid][pMuted] == 1) SCM(playerid, 0xff6600AA, "Доступ в чат заблокирован. Узнать время до окончания бана чата {66cc33}/time");
		{
		    SetPlayerChatBubble(playerid, "Пытается что-то сказать...", 0xFF0000AA, 10.0, 2000);
		}
		if(login_check[playerid] == true && Player[playerid][pMuted] == 0)
		{
			new Float:Posw[3];
			GetPlayerPos(playerid,Posw[0],Posw[1],Posw[2]);
			new string[128];
			foreach(Player, i)
			{
				if(!IsPlayerInRangeOfPoint(i,23.0,Posw[0],Posw[1],Posw[2]) || !IsPlayerConnected(i))continue;
				if(GetPlayerInterior(playerid) != GetPlayerInterior(i) || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(i))continue;
				if(!strcmp(text, ")", true))
				{
					f("%s улыбается", Player[playerid][pName]);
					StatusMessage(playerid, string, "улыбается");
     				return 0;
				}
				if(!strcmp(text, "))", true))
				{
					f("%s смеётся", Player[playerid][pName]);
					StatusMessage(playerid, string, "смеётся");
					return 0;
				}
				if(!strcmp(text, "=0", true))
				{
					f("%s удивился", Player[playerid][pName]);
					StatusMessage(playerid, string, "удивился");
     				return 0;
				}
				if(!strcmp(text, "(", true))
				{
					f("%s расстроился", Player[playerid][pName]);
					StatusMessage(playerid, string, "расстроился");
     				return 0;
				}
				if(!strcmp(text, "((",true))
				{
					f("%s сильно расстроился", Player[playerid][pName]);
					StatusMessage(playerid, string, "сильно расстроился");
					ApplyAnimation(playerid, "GRAVEYARD","mrnF_loop",4.1,0,0,0,0,0,1);
     				return 0;
				}
				if(Player[playerid][pCall] == false)
				{
					if(Player[i][pChat] == 0)
					{
					    switch(Player[playerid][pMember])
						{
							case 0: f("- %s {FFFFFF}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 1: f("- %s {009900}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 2: f("- %s {CC00FF}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 3: f("- %s {00F5FF}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 4: f("- %s {ffcd00}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 5: f("- %s {6666ff}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 6: f("- %s {0000FF}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 7: f("- %s {ff6600}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 8: f("- %s {996633}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 9: f("- %s {d2ff00}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 10: f("- %s {993366}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 11: f("- %s {bb0000}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 12: f("- %s {007575}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 13: f("- %s {ff6666}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 14: f("- %s {6495ED}(%s)[%i]", text ,Player[playerid][pName], playerid);
						}
						switch(Player[playerid][pCover])
						{
							case 1: f("- %s {009900}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 2: f("- %s {CC00FF}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 3: f("- %s {00F5FF}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 4: f("- %s {ffcd00}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 5: f("- %s {6666ff}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 6: f("- %s {993366}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 7: f("- %s {bb0000}(%s)[%i]", text ,Player[playerid][pName], playerid);
							case 8: f("- %s {007575}(%s)[%i]", text ,Player[playerid][pName], playerid);
						}
						if(GetPVarInt(playerid, "MASKON") == 1) f("- %s {4F4F4F}(%s)[%i]", text ,Player[playerid][pName], playerid);
						SCM(i,0xcececeAA,string);
					}
					if(Player[i][pChat] == 1)
					{
						f("%s[%d]: {cecece}%s",Player[playerid][pName], playerid,text);
						SCM(i,GetPlayerColor(playerid),string);
					}
					SetPlayerChatBubble(playerid, text, 0xFFFAFAAA, 15.0, 5000);
					anti_cheat[playerid][pFlood] = true;
					if(Player[playerid][pPatronPlayer] == 0 && Tazer[playerid] == false)
					{
						if(Player[playerid][pEat] == 0 && PlayerStartJob[playerid] == false)
						{
							if(Player[playerid][pAnim] == false)
							{
								ApplyAnimation(playerid, "PED", "IDLE_CHAT", 4.1, 1, 1, 1, 1, 1),Player[playerid][pChatTim] = 3;
							}
						}
					}
					if(Tazer[playerid] == true) ApplyAnimation(playerid, "CRACK", "crckdeth2", 4.0, 1, 0, 0, 0, 0);
					if(Player[playerid][pEat] >= 1 || PlayerStartJob[playerid] == true) ApplyAnimation(playerid, "CARRY","crry_prtial",4.0,1,0,0,1,1,1);
				}
			}
			if(Player[playerid][pCall] == true)
			{
				f("[Тел.] %s: %s",Player[playerid][pName], text);
				SCM(Player[playerid][pCallingID], COLOR_YELLOW, string);
				f("[Тел.] %s: %s",Player[playerid][pName], text);
				TextSCM(playerid, 0x4F4F4FAA, string, 5);
			}
		}
	}
	return 0;
}
