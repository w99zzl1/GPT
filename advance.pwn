CMD:setleader(playerid, params[])
{
    if (Player[playerid][pAdmin] < 5) return 1;

    new targetid, orgid, skinid;
    if (sscanf(params, "ddd", targetid, orgid, skinid)) 
        return SCM(playerid, COLOR_GRAD, "Используй: /setleader [ID игрока] [ID организации] [ID скина]");

    if (!IsPlayerConnected(targetid)) 
        return SCM(playerid, COLOR_GRAD, "Такого игрока нет");

    if (orgid < 1 || orgid > 14) 
        return SCM(playerid, COLOR_GRAD, "ID организации должен быть от 1 до 14");

    if (skinid < 1 || skinid > 311 || skinid == 74) 
        return SCM(playerid, COLOR_GRAD, "Некорректный ID скина");

    if (Player[targetid][pMember] >= 1) 
        return SCM(playerid, COLOR_GRAD, "Этот игрок уже состоит в организации");

    if (Player[targetid][pWarn] >= 1 || Player[targetid][pAdmin] >= 1) 
        return SCM(playerid, COLOR_GRAD, "Игрок не может быть лидером из-за предупреждений/админки");

    if (Player[targetid][pLevel] < 3) 
        return SCM(playerid, COLOR_GRAD, "Для лидерства у игрока должен быть уровень 3 или выше");

    // SQL-запрос на проверку наличия лидера в организации
    new query[128];
    if (orgid != 9) 
        format(query, sizeof(query), "SELECT * FROM leaders WHERE idorg = '%i' AND unit = '0'", orgid);
    else 
        format(query, sizeof(query), "SELECT * FROM leaders WHERE idorg = '%i' AND unit = '1'", orgid);

    new Cache:result = mysql_query(connect_mysql, query, true);
    new rows, fields;
    cache_get_data(rows, fields);
    cache_delete(result);

    if (rows > 0) 
        return SCM(playerid, COLOR_GRAD, "У этой организации уже есть лидер");

    // Назначение лидера
    Player[targetid][pMember] = orgid;
    Player[targetid][pLeader] = orgid;
    Player[targetid][pUnit] = (orgid == 9) ? 1 : 0;
    Player[targetid][pWork] = 0;
    Player[targetid][pRang] = 10;
    Player[targetid][pSkinOrg] = skinid;
    SetPlayerSkin(targetid, skinid);

    // Уведомления
    format(query, sizeof(query), "%s[%d] назначил Вас лидером организации \"%s\"", 
        Player[playerid][pName], playerid, GetGangName(orgid));
    SCM(targetid, COLOR_YELLOW, query);

    format(query, sizeof(query), "На пост лидера организации \"%s\" назначен %s[%d]", 
        GetGangName(orgid), Player[targetid][pName], targetid);
    SCM(playerid, COLOR_YELLOW, query);

    format(query, sizeof(query), "[Внимание] %s[%i] назначил игрока %s[%i] лидером организации \"%s\"", 
        Player[playerid][pName], playerid, Player[targetid][pName], targetid, GetGangName(orgid));
    SendAdminMessage(0xFF0000AA, query);

    SColor(targetid);

    // Боевые стили
    switch (orgid) {
        case 10: SetPlayerFightingStyle(targetid, FIGHT_STYLE_KNEEHEAD);
        case 11: SetPlayerFightingStyle(targetid, FIGHT_STYLE_KUNGFU);
        case 12: SetPlayerFightingStyle(targetid, FIGHT_STYLE_BOXING);
    }

    // SQL-запрос на добавление в таблицу лидеров
    format(query, sizeof(query), 
        "INSERT INTO leaders (idorg, nickname, organization, unit) VALUES ('%i', '%s', '%s', '%i')", 
        orgid, Player[targetid][pName], GetGangName(orgid), Player[targetid][pUnit]);
    mysql_query(connect_mysql, query, false);

    SaveAll(targetid);
    return 1;
}
CMD:sellgun(playerid, params[])
