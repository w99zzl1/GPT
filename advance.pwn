				if(params[1] == 1) Player[params[0]][pLicDrive] = 0;
				if(params[1] == 2) Player[params[0]][pLicGun] = 0;
			}
			if(Player[playerid][pMember] == 9)
			{
				if(Player[playerid][pUnit] == 2 || Player[playerid][pUnit] == 3 || Player[playerid][pUnit] == 4)
