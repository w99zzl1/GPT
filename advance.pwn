CMD:exit(playerid)
{
	for(new i = 0; i < sizeof(HouseInfo); i++)
	{
		if(PlayerToPoint(3, playerid,HouseInfo[i][ExitIntX], HouseInfo[i][ExitIntY], HouseInfo[i][ExitIntZ]))
		{
			if(GetPlayerVirtualWorld(playerid) == HouseInfo[i][hWorld])
			{
				SetPlayerInterior(playerid,0);
				SetPlayerVirtualWorld(playerid,0);
				intertest[playerid] = 0;
				SetPlayerPos(playerid,HouseInfo[i][ExitX],HouseInfo[i][ExitY],HouseInfo[i][ExitZ]);
			}
		}
	}
	return 1;
}
