CMD:job(playerid)
{
	if(GetPlayerState(playerid) == 2)
	{
		new jobcar = GetPlayerVehicleID(playerid);
		if(IsBus(playerid))
		{
			if(Player[playerid][CarJobID] != jobcar) return 1;
			if(Player[playerid][pWork] == 1 && Player[playerid][BusTarif] > 0) Player[playerid][CarJobID] = jobcar;
			if(Player[playerid][pWork] == 1 && Player[playerid][BusTarif] > 0) SPD(playerid,27,DIALOG_STYLE_MSGBOX,"{FFCD00}Диспетчер автопарка", "{FFFFFF}Вы уверены что хотите завершить рабочий день?", "Да", "Нет");
			if(Player[playerid][pWork] == 1 && Player[playerid][BusTarif] == 0) SPD(playerid, 25, DIALOG_STYLE_LIST,"{FFCD00}Выберите маршрут", "{FFFFFF}1. Вокзал - Шахта\n2. Городской Лос-Сантос\n3. Лос-Сантос - Лас-Вентурас", "Начать", "Отмена");
		}
		if(IsTaxi(playerid))
		{
			if(Player[playerid][pWork] == 2 && Player[playerid][TaxiTarif] > 0) Player[playerid][CarJobID] = jobcar;
			if(Player[playerid][pWork] == 2 && Player[playerid][TaxiTarif] > 0) SPD(playerid,27,DIALOG_STYLE_MSGBOX,"{0066FF}Такси","Вы точно хотите закончить свой рабочий день ?","Да","Нет");
			if(Player[playerid][pWork] == 2 && Player[playerid][TaxiTarif] == 0) SPD(playerid,26,DIALOG_STYLE_INPUT,"{0066FF}Такси","Введите тариф для\nвашего такси (от 1 до 200$):","Ок","Закрыть");
		}
		if(jobcar == MehanLS[0] || jobcar == MehanLS[1] || jobcar == MehanLS[2] || jobcar == MehanLS[3] || jobcar == MehanLS[4] || jobcar == MehanLV[0] || jobcar == MehanLV[1] || jobcar == MehanLV[2] || jobcar == MehanLV[3] || jobcar == MehanLV[4] || jobcar == MehanSF[0] ||jobcar == MehanSF[1] || jobcar == MehanSF[2] || jobcar == MehanSF[3] || jobcar == MehanSF[4])
		{
			if(Player[playerid][pWork] == 6 && Player[playerid][Mehan] == true) Player[playerid][CarJobID] = jobcar;
			if(Player[playerid][pWork] == 6 && Player[playerid][Mehan] == true) SPD(playerid,27,DIALOG_STYLE_MSGBOX,"{0066FF}Автомеханик","Вы точно хотите закончить свой рабочий день ?","Да","Нет");
			if(Player[playerid][pWork] == 6 && Player[playerid][Mehan] == false) SPD(playerid,28,DIALOG_STYLE_MSGBOX,"{0066FF}Автомеханик","Вы хотите начать свой рабочий день ?","Да","Нет");
		}
		if(IsAir(playerid))
		{
			if(Player[playerid][pWork] == 9 && Player[playerid][AirTarif] > 0) Player[playerid][CarJobID] = jobcar;
			if(Player[playerid][pWork] == 9 && Player[playerid][AirTarif] > 0) SPD(playerid,27,DIALOG_STYLE_MSGBOX,"{0066FF}Пилот самолёта","Вы точно хотите закончить свой рабочий день ?","Да","Нет");
			if(Player[playerid][pWork] == 9 && Player[playerid][AirTarif] == 0) SPD(playerid,23,DIALOG_STYLE_LIST,"{0066FF}Пилот самолёта | Выберите маршрут","1. Компания Airplane","Ок","Закрыть");
		}
	}
	return 1;
}
CMD:e(playerid)
{
 	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return 1;
	if(!IsABMX(playerid))
	{
		if(GetPlayerState(playerid) != 2) return 0;
		new vid = GetPlayerVehicleID(playerid);
		if(VehicleFuel[vid] <= 0)
		{
			VehicleFuel[vid] = 0;
			GameTextForPlayer(playerid, "~r~No Fuel", 1000, 4);
			SetVehicleParamsEx(vid,0,VehInfo[vid][pLights],VehInfo[vid][pAlarm],VehInfo[vid][pDoors],VehInfo[vid][pBonnet],VehInfo[vid][pBoot],VehInfo[vid][pObjective]);
			return 1;
		}
		GetVehicleParamsEx(vid,VehInfo[vid][pEngine],VehInfo[vid][pLights],VehInfo[vid][pAlarm],VehInfo[vid][pDoors],VehInfo[vid][pBonnet],VehInfo[vid][pBoot],VehInfo[vid][pObjective]);
		if(VehInfo[vid][pEngine] == 0) SetVehicleParamsEx(vid,1,VehInfo[vid][pLights],VehInfo[vid][pAlarm],VehInfo[vid][pDoors],VehInfo[vid][pBonnet],VehInfo[vid][pBoot],VehInfo[vid][pObjective]);
		if(VehInfo[vid][pEngine] == 1) SetVehicleParamsEx(vid,0,VehInfo[vid][pLights],VehInfo[vid][pAlarm],VehInfo[vid][pDoors],VehInfo[vid][pBonnet],VehInfo[vid][pBoot],VehInfo[vid][pObjective]);
		if(VehInfo[vid][pEngine] == 0) VehInfo[vid][pEngine] = 1;
		if(VehInfo[vid][pEngine] == 1) VehInfo[vid][pEngine] = 0;
	}
	return 1;
}
