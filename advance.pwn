						{
							if(Player[params[0]][pLicDrive] == 0) return SCM(playerid, COLOR_GRAD, "У игрока нет этой лицензии.");
						}
						if(params[1] == 2)
						{
							if(Player[playerid][pRang] != 7) return SCM(playerid, COLOR_GRAD, "Лицензию на оружие можно отбирать только на должности старшего лицензёра. (7 ранг)");
							if(Player[params[0]][pLicGun] == 0) return SCM(playerid, COLOR_GRAD, "У игрока нет этой лицензии.");
						}
						new rang[64];
						rang = RankName(playerid);
						format(query,sizeof(query),  "%s %s забрал у Вас лицензию на %s. Причина: %s", rang,Player[playerid][pName],licensies,params[2]);
						SCM(params[0], 0x0099FFAA, query);
						format(query,sizeof(query),  "[!] %s %s забрал у %s лицензию на %s. Причина: %s", rang,Player[playerid][pName],Player[params[0]][pName],licensies,params[2]);
						SendMemberMessage(playerid,0x99FF00AA, query);
						if(params[1] == 1) Player[params[0]][pLicDrive] = 0;
