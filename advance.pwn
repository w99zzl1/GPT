				GetPlayerTransfer1[params[0]] = playerid;
				GetPlayerOption1[params[0]] = params[1];
				GetPlayerOption2[params[0]] = params[2];
				new licensies[64];
				if(params[1] == 1) licensies = "базовую лицензию на транспорт";
				if(params[1] == 2) licensies = "профессиональную лицензию на транспорт";
				if(params[1] == 3) licensies = "лицензию на оружие";
