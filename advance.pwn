stock BanIP(ip[], days)
{
	new mquery[180];
	mysql_format(connect_mysql, mquery, sizeof(mquery), "INSERT INTO ip_bans (ip_ban_address, ip_ban_date, ip_ban_expdate) VALUES ('%e', NOW(), NOW() + INTERVAL %d DAY)", ip, days);
	mysql_pquery(connect_mysql, mquery);
	return 1;
}
stock IsIPBanned(ip[])
{
	new mquery[180], bool:isBanned;
	isBanned = false;
	mysql_format(connect_mysql, mquery, sizeof(mquery), "SELECT * FROM ip_bans WHERE ip_ban_address = '%e' AND ip_ban_expdate > NOW() LIMIT 1", ip);
	new Cache: resultCache = mysql_query(connect_mysql, mquery, true);
	if(cache_num_rows() > 0) isBanned = true;
	cache_delete(resultCache);
	return isBanned;
}
stock UnBanIP(ip[])
{
	new mquery[180];
	mysql_format(connect_mysql, mquery, sizeof(mquery), "DELETE FROM ip_bans WHERE ip_ban_address = '%e' LIMIT 1", ip);
	mysql_pquery(connect_mysql, mquery);
}
stock strreplace(string[], find, replace)
{
	for(new i=0; string[i]; i++)
 	{
  		if(string[i] == find)
        {
        	string[i] = replace;
        }
	}
}
