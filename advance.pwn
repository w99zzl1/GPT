		}
	}
	return 1;
}
CMD:unrent(playerid)
{
	if(Player[playerid][pRentCar] == 0) return SCM(playerid,COLOR_GRAD, !"У Вас нет договора аренды транспорта");
	if(GetPlayerState(playerid) == 2)
	{
		if(Player[playerid][pRentCar] == GetPlayerVehicleID(playerid)) RemovePlayerFromVehicle(playerid);
	}
	new rentcar = Player[playerid][pRentCar];
	GetVehicleParamsEx(rentcar,VehInfo[rentcar][pEngine],VehInfo[rentcar][pLights],VehInfo[rentcar][pAlarm],VehInfo[rentcar][pDoors],VehInfo[rentcar][pBonnet],VehInfo[rentcar][pBoot],VehInfo[rentcar][pObjective]);
	if(VehInfo[rentcar][pDoors] == 1)
	{
		VehInfo[rentcar][pDoors] = 0;
		SetVehicleParamsEx(rentcar,VehInfo[rentcar][pEngine],VehInfo[rentcar][pLights],VehInfo[rentcar][pAlarm],0,VehInfo[rentcar][pBonnet],VehInfo[rentcar][pBoot],VehInfo[rentcar][pObjective]);
	}
	Player[playerid][pRentCar] = 0;
	SCM(playerid, 0xFFFF00FF, !"Договор аренды транспорта расторгнут");
	return 1;
}
