CMD:respv(playerid, params[])
{
   	if(Player[playerid][pAdmin] < 3) return 1;
	{
		if(IsPlayerInAnyVehicle(playerid))
		{
			SetVehicleToRespawn(GetPlayerVehicleID(playerid));
			SCM(playerid,0x00FF00AA, !"Текущий транспорт респавнен");
			return 1;
		}
		new Float: xa, Float: xy, Float: xz;
		if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_GRAD, !"Используй: /respv [радиус] или [-1]");
		{
			if(params[0] < -1 || params[0] > 80) return SCM(playerid, COLOR_GRAD, !"Радиус от -1 до 80");
			GetPlayerPos(playerid,xa,xy,xz);
			for(new i = 0; i < MAX_VEHICLES; i++)
			{
				if(VehToPoint(params[0],i,xa,xy,xz))
				{
					SetVehicleToRespawn(i);
					if(createcar[creates] == i) DestroyVehicle(i),creates --;
				}
			}
			SCM(playerid, 0x00FF00AA, !"Машины в указанном радиусе были респавнены");
		}
	}
	return 1;
}
CMD:vec(playerid, params[])
{
	if(Player[playerid][pAdmin] < 4) return 1;
	{
	    if(sscanf(params, "ddd", params[0], params[1], params[2])) return SCM(playerid, COLOR_GREY, !"Используйте: /vec [id транспорта] [цвет №1] [цвет №2]");
		if(params[0] < 400 || params[0] > 611) return SCM(playerid, COLOR_GRAD, !"Номер транспортного средства не может быть ниже 400 или выше 611");
		if(params[1] < 0 || params[1] > 255) return SCM(playerid, COLOR_GRAD, !"Номер цвета от 0 до 255");
		if(params[2] < 0 || params[2] > 255) return SCM(playerid, COLOR_GRAD, !"Номер цвета от 0 до 255 ");
		{
			new Float:X,Float:Y,Float:Z;
			GetPlayerPos(playerid, X,Y,Z);
			if(params[0] != 510) createcar[creates] = CreateVehicle(params[0], X,Y+2,Z, 0.0, params[1], params[2], 60000);
			if(params[0] == 510) createcar[creates] = CreateVehicle(params[0], X,Y+2,Z, 0.0, params[1], params[2], 30);
			VehicleFuel[createcar[creates]] = 40;
			vehcreat[creatcar[playerid]] = 2;
			if(params[0] == 510) VehicleFuel[createcar[creates]] = 999;
			VehInfo[createcar[creates]][pEngine] = 0;
			VehInfo[createcar[creates]][pLights] = 0;
			VehInfo[createcar[creates]][pAlarm] = 0;
			VehInfo[createcar[creates]][pDoors] = 0;
			VehInfo[createcar[creates]][pBonnet] = 0;
			VehInfo[createcar[creates]][pBoot] = 0;
			VehInfo[createcar[creates]][pObjective] = 0;
			SetVehicleParamsEx(createcar[creates],VehInfo[createcar[creates]][pEngine]
			,VehInfo[createcar[creates]][pLights],VehInfo[createcar[creates]]
			[pAlarm],VehInfo[createcar[creates]][pDoors],VehInfo[createcar[
			creates]][pBonnet],VehInfo[createcar[creates]][pBoot],VehInfo[
			createcar[creates]][pObjective]);
			creates++;
		}
	}
	return 1;
}
