	for(new i; i < sizeof(Obj_FloorDoors); i ++)
	{
		if(!IsPlayerInAnyVehicle(playerid))
		{
			new Float:pos[3];
			GetPlayerPos(playerid, pos[0], pos[1], pos[2]);
			if(pos[1] < -1301.4 && pos[1] > -1303.2417 && pos[0] < 1786.2131 && pos[0] > 1784.1555)  ShowElevatorDialog(playerid);
			else
			{
				if(pos[1] > -1301.4 && pos[1] < -1299.1447 && pos[0] < 1785.6147 && pos[0] > 1781.9902)
				{
					new s=20;
					while(pos[2] < GetDoorsZCoordForFloor(i) + 3.5 && s > 0) s --;
					if(s == 0 && pos[2] < GetDoorsZCoordForFloor(0) + 2.0)  s = -1;
					if(s <= 19)
					{
						CallElevator(playerid, s + 1);
						GameTextForPlayer(playerid, "~g~WAIT...", 1000, 1);
					}
				}
			}
		}
	}
	return 1;
}
