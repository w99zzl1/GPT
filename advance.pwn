				SCM(playerid, 0x1e90ffAA, query);
				format(query,sizeof(query),  "%s предлагает Вам купить %s за %d$", Player[playerid][pName], licensies,params[2]);
				SCM(params[0],  0x3196FFFF, query);
				SCM(params[0], COLOR_WHITE, "Нажмите {00CB00}Y {FFFFFF}чтобы купить лицензию или {FF6900}N {FFFFFF}для отказа");
