	{
		new totalLS, totalSF, totalLV;
		foreach(Player, i)
		{
			if(IsPlayerConnected(i))
			{
				if(Player[i][pRadio] == 1)
				{
					totalLS ++;
				}
				if(Player[i][pRadio] == 2)
				{
					totalSF ++;
				}
				if(Player[i][pRadio] == 3)
				{
					totalLV++;
				}
			}
		}
		new string[64];
		f("{FFFFFF}Радио ЛС:\t%d\nРадио СФ:\t%d\nРадио ЛВ:\t%d", totalLS, totalSF, totalLV);
