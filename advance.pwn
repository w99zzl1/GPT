		if(login_check[i] == true && Player[i][pAFK] == true)
		{
			SCM(i,0xFFCC00AA, "Для получения зарплаты Вы не должны находиться на паузе.");
			Player[i][pInGameTime] = 0;
		}
 		if(login_check[i] == true)
		{
			if(Player[i][pInGameTime] >= 20 && Player[i][pAFK] == false)
			{
				PlayerPlaySound(i, 6400, 0.0, 0.0, 0.0);
				if(Player[i][pZakon] < 100 && Player[i][pZakon] > -100) Player[i][pZakon] ++;
				switch(Player[i][pAdmin])
				{
					case 1: SCM(i, COLOR_WHITE, "Админ - зарплата: {00cc00}3000$"),Player[i][pBank] += 3000;
					case 2: SCM(i, COLOR_WHITE, "Админ - зарплата: {00cc00}4000$"),Player[i][pBank] += 4000;
					case 3: SCM(i, COLOR_WHITE, "Админ - зарплата: {00cc00}5000$"),Player[i][pBank] += 5000;
					case 4: SCM(i, COLOR_WHITE, "Админ - зарплата: {00cc00}6000$"),Player[i][pBank] += 6000;
					case 5: SCM(i, COLOR_WHITE, "Админ - зарплата: {00cc00}7000$"),Player[i][pBank] += 7000;
				}
				if(Player[i][pAdmin] >= 1)
				{
					Player[i][pMember] = 0;
					Player[i][pLeader] = 0;
					Player[i][pUnit] = 0;
					Player[i][pRang] = 0;
					SetPlayerColor(i,COLOR_WHITE);
				}
				if(Player[i][pMember] >= 0 && Player[i][pAdmin] == 0)
				{
					CASH[i] = CheckCash(i);
					if(Player[i][pMember] == 6 && Player[i][pUnit] == 4)
					{
						switch(Player[i][pRang])
						{
							case 10: CASH[i] = 4500;
							case 9: CASH[i] = 3900;
							case 8: CASH[i] = 3700;
							case 7: CASH[i] = 3500;
							case 6: CASH[i] = 3250;
							case 5: CASH[i] = 2750;
							case 4: CASH[i] = 2400;
							case 3: CASH[i] = 1900;
							case 2: CASH[i] = 1450;
							case 1: CASH[i] = 1330;
						}
					}
					if(Player[i][pMember] == 9 && Player[i][pUnit] == 1)
					{
						switch(Player[i][pRang])
						{
							case 10: CASH[i] = 5200;
							case 9: CASH[i] = 4600;
							case 8: CASH[i] = 3200;
							case 7: CASH[i] = 3200;
							case 6: CASH[i] = 3200;
							case 5: CASH[i] = 3200;
							case 4: CASH[i] = 3200;
							case 3: CASH[i] = 1750;
							case 2: CASH[i] = 1350;
							case 1: CASH[i] = 940;
						}
					}
					CASH[i] += Player[i][pCash];
					Player[i][pCash] = 0;
					Player[i][pBank] += CASH[i];
				}
				if(CASH[i] > 0)
				{
				format(query, sizeof(query), "Зарплата: {00CC00}%d$", CASH[i]);
				SCM(i, COLOR_WHITE, query);
				}
				if(CASH[i] <= 0 && Player[i][pAdmin] == 0) SCM(i, COLOR_WHITE, "Зарплата: {FF6347}0$");
				if(login_check[i] == true && Player[i][pAFK] == false) format(query, sizeof(query), "Текущий баланс счёта: {00CC00}%d$", Player[i][pBank]);
				if(Player[i][pBank] == 0 && Player[i][pAFK] == false) format(query, sizeof(query), "Текущий баланс счёта: {FF6600}%d$", Player[i][pBank]);
				SCM(i, COLOR_WHITE, query);
				Player[i][pInGameTime] = 0;
				CASH[i] = 0;
			}
			SCM(i,COLOR_WHITE, "______________________");
			if(Player[i][pLoto] == true)
			{
				new string[150], totalNums = 0;
				f("Начинаем лотерейный розыгрыш. Ваше счастливое число: %d%d%d",GetPVarInt(i, "LOTO1"),GetPVarInt(i, "LOTO2"),GetPVarInt(i, "LOTO3"));
