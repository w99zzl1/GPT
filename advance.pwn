stock DebtorSellHouse(h)
{
    static const fmt_query[] = "UPDATE `houses` SET `Owner` = '%s', `Owned` = '0', `RentTime` = '1', `Updates` = '0', `hStoreX` = '0', `hStoreY` = '0',`hStoreZ` = '0', `hStoreMetall` = '0', `hStoreDrugs` = '0', `hStoreGun` = '0', `hStoreAmmo` = '0', `hStoreSkin` = '0', `evict` = '0' WHERE `ID` = '%d'";
	format(query, sizeof(query), fmt_query, HouseInfo[h][hOwner], HouseInfo[h][houseid]);
	mysql_query(connect_mysql, query);
    new upgrademoney;
	switch(HouseInfo[h][hUpdate])
	{
	    case 1: upgrademoney = 8000/100*60;
	    case 2: upgrademoney = (8000+14500)/100*60;
	    case 3:	upgrademoney = (8000+14500+20000)/100*60;
	    case 4: upgrademoney = (8000+14500+20000+55000)/100*60;
	    case 5: upgrademoney = (8000+14500+20000+55000+60000)/100*60;
	}
	new giverhm[25];
	format(giverhm, sizeof(giverhm), "%d,%d,0", HouseInfo[h][hPrice]/100*75, upgrademoney);//ДОРАБОТАТЬ (добавить возврат денег за автомобиль)
	format(query, sizeof(query), "UPDATE `accounts` SET `ReturnMoney` = '%s' WHERE `house` = '%d'", giverhm, HouseInfo[h][houseid]);
	mysql_query(connect_mysql, query);
	foreach(new i:Player)
	{
		if(strcmp(Player[i][pName], HouseInfo[h][hOwner], false, 24)) continue;
		Player[i][pHouse] = 999;
		Player[i][pSetSpawn] = 1;
		strmid(Player[i][ReturnMoney], giverhm, 0, 24);
		break;
	}
    HouseInfo[h][hOwned] = 0;
	strdel(HouseInfo[h][hOwner], 0, MAX_PLAYER_NAME);
	HouseInfo[h][hRentTime] = 1;
	HouseInfo[h][StoreX] = 0;
	HouseInfo[h][StoreY] = 0;
	HouseInfo[h][StoreZ] = 0;
	HouseInfo[h][StoreMetall] = 0;
	HouseInfo[h][StoreDrugs] = 0;
	HouseInfo[h][StoreGun] = 0;
	HouseInfo[h][StoreAmmo] = 0;
	HouseInfo[h][StoreSkin] = 0;
	if(HouseInfo[h][hUpdate] >= 2)
	{
	    DestroyDynamicPickup(HouseInfo[h][hHeal]);
	}
	if(HouseInfo[h][hUpdate] >= 5)
	{
	    DestroyDynamic3DTextLabel(HouseInfo[h][StoreInfoHouse]);
	}
	HouseInfo[h][hUpdate] = 0;
	DestroyDynamicMapIcon(HouseInfo[h][hMIcon]);
    DestroyDynamicPickup(HouseInfo[h][hPickup]);
    HouseInfo[h][hPickup] = CreateDynamicPickup(1273, 1, HouseInfo[h][ExitIntX], HouseInfo[h][ExitIntY], HouseInfo[h][ExitIntZ], -1);
    HouseInfo[h][hMIcon] = CreateDynamicMapIcon(HouseInfo[h][ExitIntX], HouseInfo[h][ExitIntY], HouseInfo[h][ExitIntZ], 31, 0, -1, -1, -1, 180);
    static const fmt_query2[] = "UPDATE `accounts` SET `houses` = '999', `spawn` = '1' WHERE `houses` = '%d'";
    format(query, sizeof(query), fmt_query2, HouseInfo[h][houseid]);
	mysql_query(connect_mysql, query);
}

