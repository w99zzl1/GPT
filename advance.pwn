stock Exp()
{
    foreach(new i : Player)
    {
        if(login_check[i] == true)
        {
            if(Player[i][pAFK] == false && Player[i][pInGameTime] >= 20) 
                Player[i][pCheckTime] += 1; // Добавляем опыт

            if(Player[i][pWanted] >= 1) 
                Player[i][pWanted] -= 1; // Уменьшаем wanted уровень

            SetPlayerWantedLevel(i, Player[i][pWanted]); // Обновляем wanted уровень

            // Рассчитываем опыт, необходимый для следующего уровня
            new requiredExp = Player[i][pLevel] * 4;

            if(Player[i][pCheckTime] >= requiredExp)
            {
                // Повышаем уровень
                Player[i][pLevel]++;

                // Отправляем сообщение игроку
                SCM(i, 0x0066FFAA, "Поздравляем! Ваш уровень повышен");

                // Сбрасываем очки опыта
                Player[i][pCheckTime] = 0; // Очистка опыта

                // Обновляем счет игрока
                SetPlayerScore(i, Player[i][pLevel]);

                // Проверка на 4 уровень
                if(Player[i][pLevel] == 4)
                {
                    new target;
                    target = ReturnUser(Player[i][pAward]);

                    if(target >= 0 && target != INVALID_PLAYER_ID)
                    {
                        // Вознаграждение для игрока, который указал ник
                        Player[target][pMoney] += 50000;
                        format(query, sizeof(query), "Игрок %s указал Ваш ник при регистрации, Вы получили вознаграждение {00cc00}50.000$", Player[i][pName]);
