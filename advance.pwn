CMD:abadge(playerid, params[])
{
    if (Player[playerid][pAdmin] < 1) // Проверка на админку
        return SendClientMessage(playerid, 0xFF0000FF, "[Ошибка] У вас нет прав на использование!");

    // Переключение статуса значка
    if (Player[playerid][pAdminBadge] == 0)
    {
        Player[playerid][pAdminBadge] = 1;
        SetPlayerChatBubble(playerid, "Администратор\n", COLOR_LIGHTRED, 15.0, 999999999); // Бесконечное время
        SendClientMessage(playerid, 0xFF0000FF, "Вы включили значок администратора!");
    }
    else
    {
        Player[playerid][pAdminBadge] = 0;
        SetPlayerChatBubble(playerid, "", 0xFF0000FF, 15.0, 1); // Убираем значок
        SendClientMessage(playerid, 0xFF0000FF, "Вы отключили значок администратора!");
    }
    return 1;
}
