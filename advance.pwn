CMD:makeadmin(playerid, params[])
{
    if (Player[playerid][pAdmin] < 5) return 1;

    new targetid, level;
    if (sscanf(params, "ud", targetid, level)) 
        return SCM(playerid, COLOR_GRAD, "Используйте /makeadmin [id] [lvl]");

    if (!IsPlayerConnected(targetid)) 
        return SCM(playerid, COLOR_GRAD, "Игрок не найден!");

    // Назначаем новый уровень админки
    Player[targetid][pAdmin] = level;
    
    // Сохраняем данные игрока
    SaveAll(targetid);

    // Выводим сообщения в чат
    new string[128];
    
    format(string, sizeof(string), "Вы назначили игрока %s админом %d уровня", Player[targetid][pName], level);
