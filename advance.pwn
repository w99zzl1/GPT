CMD:give(playerid, params[])
{
    new string[128], give_name[24];
    if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_GREY, !"Используйте: /give [id игрока]");
    if(GetDistanceBetweenPlayers(playerid, params[0]) > 20) return SCM(playerid, COLOR_GRAD, "Игрок должен быть рядом с Вами");
    if(!IsPlayerConnected(params[0])) return SCM(playerid,COLOR_GREY, "Такого игрока нет");
    if(params[0] == playerid) return SCM(playerid, COLOR_GRAD, "Такого игрока нет");
 	GetPlayerName(playerid, give_name, sizeof(give_name));
  	f("{ffcf00}%s", give_name);
   	SPD(playerid, 101, DIALOG_STYLE_LIST, string,
   	"{FFFFFF}1. Передать металл\n\
   	2. Передать патроны\n\
   	3. Передать наркотики\n\
   	4. Передать маски\n\
   	5. Передать аптечки",
   	"Выбрать", "Закрыть");
    return 1;
}
CMD:healme(playerid)
{
	if(Medicine_Bag[playerid] == 0) return SCM(playerid,COLOR_GRAD, !"У Вас нет аптечки");
	SetPlayerChatBubble(playerid, "+ 60 HP", 0x0099FFAA, 10.0, 2000);
	GameTextForPlayer(playerid, "~b~+ 60 hp", 1000, 1);
	SCM(playerid,0x0099FFAA, !"Вы использовали аптечку.Здоровье поплнено на 60 единиц");
	Medicine_Bag[playerid] -= 1;
	Player[playerid][pHealth] += 60.0;
	SetPlayerHealth(playerid, Player[playerid][pHealth]);
	ApplyAnimation(playerid,"ped","gum_eat",4.0,0,0,0,0,0,1);
	if(Player[playerid][pHealth] > 100.0) //pAirb
	{
		Player[playerid][pHealth] = 100.0;
		SetPlayerHealth(playerid, 100.0);
	}
	return 1;
}
CMD:mask(playerid)
