CMD:rang(playerid, params[])
{
    if(Player[playerid][pMember] != 1 && Player[playerid][pRang] != 9) return SCM(playerid,COLOR_GRAD, !"Вам недоступна данная функция");
    if(sscanf(params, "dd", params[0], params[1])) return SCM(playerid, COLOR_GRAD, "Используйте /rang [id игрока] [+ или -]");
    if(!IsPlayerConnected(params[0])) return SCM(playerid,COLOR_GRAD,"Такого игрока нет");
	if(!strlen(params[1])) return SCM(playerid, COLOR_GRAD, "Используйте /rang [id игрока] [+ или -]");
	if(params[0] == playerid) return SCM(playerid, COLOR_GRAD, "Игрока с таким id нет");
	if(GetDistanceBetweenPlayers(playerid, params[0]) > 3 || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0])) return SCM(playerid, COLOR_GRAD, "Игрок слишком далеко от Вас");
	if(Player[params[0]][pLeader] >= 1 && Player[params[0]][pAdmin] == 0) return SCM(playerid, COLOR_GRAD, "Игрока с таким id нет");
	if(Player[playerid][pMember] != Player[params[0]][pMember]) return SCM(playerid, COLOR_GRAD, "Этот игрок не из Вашей организации");
	if(Player[playerid][pUnit] > 0)
	{
		if(Player[playerid][pUnit] != Player[params[0]][pUnit]) return SCM(playerid, COLOR_GRAD, "Этот игрок не из Вашего подразделения");
	}
	if(strcmp(params[1],"+",true) == 0)
	{
	    if(Player[params[0]][pRang] >= 9) return SCM(playerid, COLOR_GRAD, "Нельзя повысить игрока выше 9 ранга");
		if(Player[params[0]][pRang] == 8 && Player[playerid][pRang] == 9) return SCM(playerid, COLOR_GRAD, "Нельзя повысить игрока выше 8 ранга");
		if(Player[params[0]][pRang] == 9 && Player[playerid][pRang] == 10) return SCM(playerid, COLOR_GRAD, "Нельзя повысить игрока выше 9 ранга");
		Player[params[0]][pRang] ++;
		format(query, sizeof(query), "Ваш ранг в организации был повышен до %i (%s)", Player[params[0]][pRang],RankName(params[0]));
		SCM(params[0], 0x0099FFAA, query);
		format(query, sizeof(query), "Вы повысили игрока %s в должности до %i (%s)", Player[params[0]][pName],Player[params[0]][pRang],RankName(params[0]));
		SCM(playerid, 0x0099FFAA, query);
	}
	if(strcmp(params[1],"-",true) == 0)
	{
		if(Player[params[0]][pRang] == 1) return SCM(playerid, COLOR_GRAD, "Нельзя понизить игрока ниже 1 ранга");
		Player[params[0]][pRang] --;
		format(query, sizeof(query), "Ваш ранг в организации был понижен до %i (%s)", Player[params[0]][pRang],RankName(params[0]));
		SCM(params[0], 0xFF6600AA, query);
		format(query, sizeof(query), "Вы понизили игрока %s в должности до %i (%s)", Player[params[0]][pName],Player[params[0]][pRang],RankName(params[0]));
		SCM(playerid, 0x0099FFAA, query);
	}
	return 1;
}
