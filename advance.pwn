CMD:clear(playerid, params[])
{
    if(Player[playerid][pMember] != 6) return SCM(playerid,COLOR_GRAD, !"Вы не состоите в МВД");
	if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_GREY, "Используйте: /clear [id игрока]");
	if(!IsPlayerConnected(params[0])) return SCM(playerid,COLOR_GREY,"Такого игрока нет");
	if(GetDistanceBetweenPlayers(playerid, params[0]) > 8 || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0])) return SCM(playerid, COLOR_GRAD, "Игрок слишком далеко от Вас");
	if(Player[params[0]][pWanted] == 0) return SCM(playerid, COLOR_GRAD, "Этот игрок не в розыске");
	format(query, sizeof(query), "%s %s[%d] снял розыск с %s[%d]", RankName(playerid), Player[playerid][pName],playerid, Player[params[0]][pName],params[0]);
	SendMemberMessage(playerid,0xFFD700AA,query);
	format(query, sizeof(query), "%s %s снял с Вас розыск", RankName(playerid), Player[playerid][pName]);
	SCM(params[0],0x0099FFAA,query);
	Player[params[0]][pWanted] = 0;
	SetPlayerWantedLevel(params[0], 0);
	return 1;
}
CMD:tie(playerid, params[])
{
    if(Player[playerid][pMember] == 10 || Player[playerid][pMember] == 11 || Player[playerid][pMember] == 12) return SCM(playerid,COLOR_GRAD, !"Вы не состоите в мафии");
	if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_GREY, "Используйте: /tie [id игрока]");
	if(!IsPlayerConnected(params[0])) return SCM(playerid,COLOR_GREY,"Такого игрока нет");
	if(!IsMafiaCarMe(params[0])) return SCM(playerid,COLOR_GRAD, "Этот игрок должен быть в машине мафии");
	if(!IsMafiaCarMe(playerid)) return SCM(playerid,COLOR_GRAD, "Вы должны быть в машине мафии");
	if(GetDistanceBetweenPlayers(playerid, params[0]) > 5 || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0])) return SCM(playerid, COLOR_GRAD, "Игрок слишком далеко от Вас");
	if(Player[params[0]][tie] == true) return SCM(playerid,COLOR_GRAD, "Этот игрок уже связан");
	format(query, sizeof(query), "%s связал руки %s",Player[playerid][pName],Player[params[0]][pName]);
	StatusMessage(playerid, query,query);
	Player[params[0]][tie] = true;
	TogglePlayerControllable(params[0], 0);
	SetCameraBehindPlayer(params[0]);
	return 1;
}
