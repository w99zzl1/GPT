    SCM(targetid, COLOR_YELLOW, string);

    return 1;
}
CMD:warn(playerid, params[])
{
    if(Player[playerid][pAdmin] < 3) return 1;
    {
    	if(sscanf(params, "ds[128]", params[0], params[1])) return SCM(playerid, COLOR_GREY, "Используй /warn [id игрока] [причина]");
