		SCMTA(COLOR_LIGHTRED, query);
		SCM(params[0], 0xff6600AA, "Для снятия предупреждений используйте команду {00cc00}/unwarn");
		SCM(params[0], 0xff6600AA, "До снятия всех предупреждений Вам запрещено вступать в организации");
		Player[params[0]][pAdmin] = 0;
		Player[params[0]][pMember] = 0;
		Player[params[0]][pRang] = 0;
		Player[params[0]][pSkinOrg] = 0;
		Player[params[0]][pUnWarn] = 0;
		Player[params[0]][pUnWarntest] = 0;
		Player[params[0]][pSetSpawn] = 0;
		SetPlayerSkin(params[0],Player[params[0]][pSkin]);
		if(Player[params[0]][pLeader] >= 1)
		{
		    new infoleadersgo[64];
			format(infoleadersgo, sizeof(infoleadersgo), "SELECT * FROM leaders WHERE idorg = '%i'", Player[params[0]][pLeader]);
			mysql_query(connect_mysql,infoleadersgo,true);
			new rows,fields;
			cache_get_data(rows, fields, connect_mysql);
			if(rows)
			{
			    new juasc[80];
				format(juasc, sizeof(juasc), "DELETE FROM leaders WHERE idorg = '%i' LIMIT 1", Player[params[0]][pLeader]);
				mysql_query(connect_mysql,juasc,false);
			}
		}
		Player[params[0]][pLeader] = 0;
		KickEx(params[0]);
		SaveAll(params[0]);
	}
	return 1;
}
CMD:unwarn(playerid, params[])
{
    if(Player[playerid][pAdmin] < 4) return 1;
	{
    	if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_GREY, "Используй /unwarn [id игрока]");
		if(!IsPlayerConnected(params[0])) return SCM(playerid,COLOR_GREY,"Такого игрока нет");
	    if(login_check[params[0]] == false) return SCM(playerid, COLOR_GRAD, "Игрока с таким id нет");
		if(Player[params[0]][pWarn] == 0) return SCM(playerid, COLOR_GRAD, "У игрока нет предупреждений");
		Player[params[0]][pWarn] -= 1;
		if(Player[params[0]][pWarn] == 0)
		{
			Player[params[0]][pUnWarn] = 0;
			Player[params[0]][pUnWarntest] = 0;
		}
		format(query, sizeof(query), "[A] %s[%i] снял 1 предупреждение у игрока %s[%i]",Player[playerid][pName],playerid,Player[params[0]][pName],params[0]);
		SendAdminMessage(COLOR_GRAD, query);
		SCM(params[0],COLOR_GREEN, "Уровень предуреждений уменьшен на 1");
		AdminLog("UNWARN","No",playerid,params[0]);
	}
	if(Player[playerid][pWarn] >= 1 && Player[playerid][pAdmin] < 3)
	{
		if(Player[playerid][pUnWarntest] == 1)
		{
			new cad = getdate();
			new old = Player[playerid][pUnWarn]-cad;
			new str[64];
			SCM(playerid,0xFF6600AA, "Автоматическая система снятия предупреждений уже была запущена");
			format(str, sizeof(str), "Все предупреждения будут сняты через %d дней.",old);
			SCM(playerid,COLOR_GREEN, str);
			return 1;
		}
		new cd = getdate();
		Player[playerid][pUnWarn] = cd+10;
		Player[playerid][pUnWarntest] = 1;
		SCM(playerid,COLOR_GREEN, "Автоматическая система снятия предупреждений запущена");
		SCM(playerid,COLOR_GREEN, "До снятия всех предупреждений осталось 10 дней");
 	}
	return 1;
}
CMD:l(playerid)
{
    if(!IsPlayerInAnyVehicle(playerid)) return 1;
 	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return 1;
	{
		new vid = GetPlayerVehicleID(playerid);
		GetVehicleParamsEx(vid,VehInfo[vid][pEngine],VehInfo[vid][pLights],VehInfo[vid][pAlarm],VehInfo[vid][pDoors],VehInfo[vid][pBonnet],VehInfo[vid][pBoot],VehInfo[vid][pObjective]);
		if(VehInfo[vid][pLights] == 0)
		{
			VehInfo[vid][pLights] = 1;
			SetVehicleParamsEx(vid,VehInfo[vid][pEngine],1,VehInfo[vid][pAlarm],VehInfo[vid][pDoors],VehInfo[vid][pBonnet],VehInfo[vid][pBoot],VehInfo[vid][pObjective]);
			PlayerPlaySound(playerid, 4604, 0.0, 0.0, 0.0);
		}
		else
		{
			VehInfo[vid][pLights] = 0;
			SetVehicleParamsEx(vid,VehInfo[vid][pEngine],0,VehInfo[vid][pAlarm],VehInfo[vid][pDoors],VehInfo[vid][pBonnet],VehInfo[vid][pBoot],VehInfo[vid][pObjective]);
			PlayerPlaySound(playerid, 4604, 0.0, 0.0, 0.0);
		}
	}
	return 1;
}
CMD:alarm(playerid)
{
	if(!IsPlayerInAnyVehicle(playerid)) return SCM(playerid, COLOR_GRAD, "Вы не за рулём");
 	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return 1;
 	{
		if(signaling[GetPlayerVehicleID(playerid)] == 60605) return signaling[GetPlayerVehicleID(playerid)] = playerid,SetPVarInt(playerid, "SIGNAL", GetPlayerVehicleID(playerid)), PlayerPlaySound(playerid, 21001, 0.0, 0.0, 0.0);
		else return signaling[GetPlayerVehicleID(playerid)] = 60605,SetPVarInt(playerid, "SIGNAL",60605), PlayerPlaySound(playerid, 21001, 0.0, 0.0, 0.0);
	}
}
