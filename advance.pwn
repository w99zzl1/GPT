		{
			if(Player[playerid][pRang] == 6 || Player[playerid][pRang] == 7)
			{
			    if(sscanf(params, "ddd", params[0], params[1], params[2])) return SCM(playerid, COLOR_GREY, "Используйте: /givelic [id игрока] [тип лицензии] [цена]");
			    if(!strlen(params[1])) SCM(playerid, COLOR_GREY, "Типы: 1 -транспорт 2-оружие");
				if(!IsPlayerConnected(params[0])) return SCM(playerid,COLOR_GREY,"Такого игрока нет");
				if(params[0] == playerid) return SCM(playerid, COLOR_GRAD, "Вы указали свой id");
				if(params[1] == 0 || params[1] > 3) return SCM(playerid, COLOR_GRAD, "Типы: 1 -транспорт 2-оружие");
				if(params[1] == 1)
				{
					if(params[2] > 10000 || params[2] < 1000) return SCM(playerid, COLOR_GRAD, "Минимальная цена на эту лицензию составляет: 1.000$");
				}
				if(params[1] == 2)
				{
					if(params[2] > 30000 || params[2] < 10000) return SCM(playerid, COLOR_GRAD, "Минимальная цена на эту лицензию составляет: 10.000$");
				}
				if(params[1] == 3)
				{
					if(params[2] > 50000 || params[2] < 30000) return SCM(playerid, COLOR_GRAD, "Минимальная цена на эту лицензию составляет: 30.000$");
				}
				if(GetDistanceBetweenPlayers(playerid, params[0]) > 5 || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0])) return SCM(playerid, COLOR_GRAD, "Игрок слишком далеко от Вас");
