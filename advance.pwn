CMD:tempwork(playerid, params[])
{
    if(Player[playerid][pAdmin] < 3) return 1;
    if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_GRAD, "Используй /tempwork [id работы]");
    if(params[0] == 0)
    {
        Player[playerid][pWork] = 0;
        Player[playerid][pMember] = 0;
        Player[playerid][pLeader] = 0;
        Player[playerid][pRang] = 0;
        Player[playerid][pUnit] = 0;
        SColor(playerid);
        SCM(playerid, 0x00FF00AA, "Вы уволились с временной работы");
    }
    else if(params[0] < 1 || params[0] > 9) return SCM(playerid, COLOR_GRAD, "Неверный ID работы");
    else
    {
        Player[playerid][pWork] = params[0];
        Player[playerid][pMember] = 0;
        Player[playerid][pLeader] = 0;
        Player[playerid][pRang] = 0;
        Player[playerid][pUnit] = 0;
        SColor(playerid);
        format(query, sizeof(query), "[A] %s[%d] пошёл на временную работу \"%s\"", Player[playerid][pName], playerid, GetJobName(Player[playerid][pWork]));
        SendAdminMessage(COLOR_GREY, query);
    }
    return 1;
}
