		{
			if(GetDistanceBetweenPlayers(playerid, params[0]) > 10 || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0])) return SCM(playerid, COLOR_GRAD, "Игрок слишком далеко от Вас");
			new licensies[10];
			if(params[1] == 1) licensies = "транспорт";
