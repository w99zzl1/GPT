CMD:ticket(playerid, params[])
{
	if(Player[playerid][pMember] >= 6 && Player[playerid][pRang] >= 4)
	{
	    if(sscanf(params, "dds[128]", params[0], params[1], params[2])) return SCM(playerid, COLOR_GREY, "Используйте /ticket [id игрока] [размер штрафа] [причина]");
		if(!IsPlayerConnected(params[0])) return SCM(playerid,COLOR_GRAD,"Такого игрока нет");
		if(!strlen(params[2])) return SCM(playerid, COLOR_GRAD, "Используйте /ticket [id игрока] [причина]");
		if(params[1] > 3000) return SCM(playerid, COLOR_GRAD, "Штраф не может быть более 3000$");
		if(Player[params[0]][pMoney] < params[1]) return SCM(playerid, COLOR_GRAD, "У игрока нет такой суммы денег");
		SetPVarInt(params[0],"ticket",1);
		SetPVarInt(playerid,"yes",1);
		yesid[playerid] = params[0];
		GetPlayerOption[params[0]] = params[1];
		GetPlayerTransfer[params[0]] = playerid;
		format(query, sizeof(query), "%s %s[%d] выписал Вам штраф на %d$. Причина: %s", RankName(playerid),Player[playerid][pName],playerid,params[1],params[2]);
		SCM(params[0], 0xFF6600AA , query);
		format(query, sizeof(query), "Вы выписали штраф для %s[%d] на %d$. Причина: %s", Player[params[0]][pName],params[0],params[1],params[2]);
		SCM(playerid, 0x3399ffAA , query);
		SCM(params[0], COLOR_WHITE, "В случае отказа от оплаты штрафа вы будете объявлены в розыск 2 уровня.");
		SCM(params[0], COLOR_WHITE, "Нажмите {03c03c}Y {FFFFFF}чтобы оплатить или {ff7518}N {FFFFFF}для отказа.");
	}
	return 1;
}
