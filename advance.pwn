	        {
	            clear_admin_combo(i);
	        }
	    }
		Player[i][pInGameTimeSecond] += 1;
		if(Player[i][pInGameTimeSecond] >= 60) Player[i][pInGameTime] += 1,Player[i][pInGameTimeSecond] = 0;
