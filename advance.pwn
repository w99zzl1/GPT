stock WarnAC(playerid)
{
	if(Player[playerid][pWarn] < 3)
	{
		Player[playerid][pWarn] += 1;
		format(query, sizeof(query), "[Античит] %s[%d] получил предупреждение [%i|3].", Player[playerid][pName], playerid, Player[playerid][pWarn]);
		SCMTA(COLOR_LIGHTRED, query);
		SCM(playerid, 0xff6600AA, "Для снятия предупреждений используйте команду {00cc00}/unwarn");
		SCM(playerid, 0xff6600AA, "До снятия всех предупреждений Вам запрещено вступать в организации");
		Player[playerid][pAdmin] = 0;
		Player[playerid][pMember] = 0;
		Player[playerid][pRang] = 0;
		Player[playerid][pSkinOrg] = 0;
		Player[playerid][pUnWarn] = 0;
		Player[playerid][pUnWarntest] = 0;
		Player[playerid][pSetSpawn] = 0;
		if(Player[playerid][pWarn] == 3)
		{
			new curdate = getdate();
			Player[playerid][pBan] = 1;
			Player[playerid][pUnBanTime] = curdate+10;
			Player[playerid][pWarn] = 0;
		}
		if(Player[playerid][pLeader] >= 1)
		{
		new qser[150];
		format(qser, sizeof(qser), "SELECT * FROM leaders WHERE idorg = '%i'", Player[playerid][pLeader]);
		new Cache: lololo = mysql_query(connect_mysql,qser,true);
		if(cache_num_rows() > 0)
		{
			format(qser, sizeof(qser), "DELETE FROM leaders WHERE idorg = '%i' LIMIT 1", Player[playerid][pLeader]);
			mysql_pquery(connect_mysql,qser);
		}
		cache_delete(lololo);
		}
		Player[playerid][pLeader] = 0;
		SetPlayerSkin(playerid,Player[playerid][pSkin]);
		KickEx(playerid);
	}
	return 1;
}
stock IsPlayerApplyAnimation(playerid, animation[])
