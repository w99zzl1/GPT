stock TextSCM(playerid,color,str[],radius)
{
	new Float:Posw[3];
	GetPlayerPos(playerid,Posw[0],Posw[1],Posw[2]);
	foreach(Player, i)
	{
		if(!IsPlayerInRangeOfPoint(i,radius,Posw[0],Posw[1],Posw[2]) || !IsPlayerConnected(i))continue;
		if(GetPlayerInterior(playerid) != GetPlayerInterior(i) || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(i))continue;
		SCM(i,color,str);
		SetPlayerChatBubble(playerid, str, color, 15.0, 3000);
	}
	return 1;
}
stock StatusMessage(playerid, str[], textbuble[])
{
