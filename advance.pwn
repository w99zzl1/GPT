				if(Player[playerid][pMoney] < GetCarPrice(na)/100*3) RemovePlayerFromVehicle(playerid);
				if(Player[playerid][pMoney] < GetCarPrice(na)/100*3) return SCM(playerid, COLOR_GRAD, !"У Вас недостаточно денег на аренду");
				Player[playerid][pRentCar] = GetPlayerVehicleID(playerid);
				Player[playerid][pMoney] -= GetCarPrice(na)/100*3;
