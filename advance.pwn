	    if(sscanf(params, "dd", params[0], params[1])) return SCM(playerid, COLOR_GRAD, !"Используйте: /admdown [id игрока] [admin lvl]");
		if(Player[params[0]][pAdmin] <= params[1])
		{
			if(Player[playerid][pAdmin] == 5) SCM(playerid, COLOR_GRAD, !"Команда действует только на понижение администраторов");
		}
		if(params[0] == playerid) return SCM(playerid, COLOR_GRAD, !"Игрока с таким id нет");
