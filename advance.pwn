CMD:dice(playerid, params[])
{
	if(login_check[playerid] == true)
	if(!PlayerToPoint(30.0,playerid,1129.6842,-1.6696,1000.6797)) return SCM(playerid, COLOR_GRAD, !"Вы должны находиться в частном казино");
    if(sscanf(params, "dd", params[0], params[1])) return SCM(playerid, COLOR_GREY, !"Используйте: /dice [id игрока] [ставка]");
	if(params[1] > 100000 || params[1] < 1) return SCM(playerid, COLOR_GRAD, !"Минимальная ставка от 1$ до 100000$");
	if(IsPlayerConnected(params[0]))
	{
		if(params[0] != INVALID_PLAYER_ID)
		{
		    if(!PlayerToPoint(30.0,params[0],1129.6842,-1.6696,1000.6797)) return SCM(playerid, COLOR_GRAD, "Игрок должен находиться в игровом зале");
			if(ban_dice[params[0]] == 1) return SCM(playerid, COLOR_GRAD, "В данный момент игрок не может играть");
			if(Player[params[0]][pMoney] < params[1]) return SCM(playerid, COLOR_GRAD, "У этого игрока нет столько денег");
			if(Player[playerid][pMoney] < params[1]) return SCM(playerid, COLOR_GRAD, "У вас нету столько денег");
			if(dice_time > gettime()) return SCM(playerid, COLOR_GRAD, "В данный момент вы не можете начать играть");
			if(params[0] == playerid) return 1;
			new string[128];
			f("Вы предложили {00CC00}%s {1e90ff}бросить кости. Ставка: {00CC00}$%i",Player[params[0]][pName],params[1]);
			SCM(playerid, 0x1e90ffAA, string);
			f("{00CC00}%s {1e90ff}предлагает бросить кости. Ставка: {00CC00}$%i",Player[playerid][pName],params[1]);
			SCM(params[0], 0x1e90ffAA, string);
			SCM(params[0], COLOR_WHITE, "Нажмите {48FC23}Y {FFFFFF}чтобы согласиться {FF0000}N {FFFFFF}для отказа");
			ban_dice[playerid] = 1;
			name_bone[params[0]] = playerid;
			money_dice[params[0]] = params[1];
		}
	}
	return 1;
}
