CMD:freeze(playerid, params[])
{
    new targetid, string[128];

    if (Player[playerid][pAdmin] < 4) 
        return 1;

    if (sscanf(params, "u", targetid)) 
        return SCM(playerid, COLOR_GRAD, "Использование: /freeze [id]");

    if (!IsPlayerConnected(targetid)) 
        return SCM(playerid, COLOR_GRAD, "Игрока нет на сервере.");

    if (GetPVarInt(targetid, "Freeze") == 0)
    {
        SetPVarInt(targetid, "Freeze", 1);
        TogglePlayerControllable(targetid, 0);
        SCM(targetid, COLOR_TBLUE, "Вы были заморожены!");
        
        format(string, sizeof(string), "[A] %s[%d] заморозил игрока %s[%d]", 
            Player[playerid][pName], playerid, Player[targetid][pName], targetid);
        SCM(playerid, COLOR_RED, string);

        PlayerPlaySound(targetid, 1055, 0.0, 0.0, 0.0); // Подходящий звук заморозки
    }
    else
    {
        DeletePVar(targetid, "Freeze");
        TogglePlayerControllable(targetid, 1);
        SCM(targetid, COLOR_LIME, "Вас разморозили!");

        format(string, sizeof(string), "[A] %s[%d] разморозил игрока %s[%d]", 
            Player[playerid][pName], playerid, Player[targetid][pName], targetid);
        SCM(playerid, COLOR_RED, string);

        PlayerPlaySound(targetid, 1056, 0.0, 0.0, 0.0); // Звук разморозки
    }

    return 1;
}
