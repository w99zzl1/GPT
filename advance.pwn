CMD:affect(playerid)
{
    if(!IsMhettor(playerid)) return SCM(playerid, COLOR_GREY, !"Вы не состоите в мафии");
    if(!GetTimeAf()) return SCM(playerid,COLOR_GRAD, !"Захват зон разрешен с 14:00 до 23:00");
    if(affect_time > 0)
    {
        new fd[128];
        if(affect_time >= 125) format(fd,128,"До следущего захвата зоны осталось %d минут(а).", floatround(affect_time/60));
        if(affect_time <= 124) format(fd,128,"До следущего захвата зоны осталось около 1 минуты.");
        SCM(playerid,COLOR_GREY,fd);
