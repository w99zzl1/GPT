CMD:deladmin(playerid, params[])
{
	if(Player[playerid][pAdmin] < 5) return 1;
    if(sscanf(params,"u",params[0])) return SCM(playerid, COLOR_GREY, "Используйте: /deladmin [ник]");
    new mquery[180], Cache: resultCache;
    new string[128];
    mysql_format(connect_mysql, mquery, sizeof(mquery), "UPDATE accounts SET Admin = '0' WHERE `Name` = '%s' AND Admin > '0'", params[0]);
    resultCache = mysql_query(connect_mysql, mquery, true);
    if(cache_affected_rows() > 0)
    {
        f("Администратор %s был снят оффлайн", params[0]);
        SCM(playerid, 0x7dd01dFF, string);
    }
    else
    {
        SCM(playerid,COLOR_WHITE, "Данный игрок не является админом");
    }
    cache_delete(resultCache);
    mysql_format(connect_mysql, mquery, sizeof(mquery), "SELECT * FROM `accounts` WHERE Name = '%e'", params[0]);
    resultCache = mysql_query(connect_mysql, mquery, true);
    if(cache_affected_rows() > 0)
    {
		f("Вы удалили права администратора с аккаунта №%d", params[0]);
		SCM(playerid, 0x00FF00AA, string);
    }
    cache_delete(resultCache);
	return 1;
}
CMD:unban(playerid, params[])
{
	if(Player[playerid][pAdmin] < 4) return 1;
	{
	    if(sscanf(params,"s[24]", params[0])) return SCM(playerid, COLOR_GREY, "Используй /unban [ник игрока]");
		new ban,lip[24];
  		new newstring[40];
		mysql_real_escape_string(params[0], newstring);
		new queryunban[80];
		format(queryunban, sizeof(queryunban), "SELECT * FROM `accounts` WHERE name = '%s'",  newstring);
		new Cache: lolos = mysql_query(connect_mysql, queryunban, true);
		new rows, fields;
		cache_get_data(rows, fields);
		if(!rows)
		{
            SCM(playerid,COLOR_GRAD,"Игрока с таким id нет");
		}
		else
		{
		    new infounban[80];
			format(infounban,sizeof(infounban), "SELECT * FROM `accounts` WHERE name = '%s' LIMIT 1", params[0]);
			new Cache: unbans = mysql_query(connect_mysql, infounban, true);
  	        cache_get_data(rows, fields, connect_mysql);
			if(rows)
			{
           		ban = cache_get_field_content_int(0, "ban");
                cache_get_field_content(0, "Lip", lip, 24);
			}
			cache_delete(unbans);
			if(ban == 0) return SCM(playerid, COLOR_GRAD, "На аккаунте нет блокировки.");
			new unbango[90];
			format(unbango, sizeof(unbango), "update accounts set ban = '0',unbantime = '0' where name = '%s'",  params[0]);
			mysql_query(connect_mysql,unbango,false);
			format(query,sizeof(query),"unbanip %s",lip);
			SendRconCommand(query);
			SendRconCommand("reloadbans");
			UnBanIP(lip);
			format(query,sizeof(query),"[A] %s[%i] разбанил игрока {1e83d5}%s {9eb34e}(аккаунт %d)",Player[playerid][pName], Player[playerid][pIDacc]);
			SendAdminMessage(COLOR_GREY, query);
			admin_combo[playerid][pUnban] ++;
			if(admin_combo[playerid][pTimer] < 1) admin_combo[playerid][pTimer] = 60;
		}
		cache_delete(lolos);
	}
	return 1;
