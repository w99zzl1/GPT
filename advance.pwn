public OnPlayerTakeDamage(playerid, issuerid, Float:amount, weaponid, bodypart)
{
	if(!IsPlayerConnected(playerid)) return 1;
	if(!IsPlayerConnected(issuerid)) return 1;
	Player[playerid][fakekill] = issuerid;
	if(Player[issuerid][pMember] == 6 && weaponid == 3)
	{
		if(Tazer[playerid] == true) return SCM(issuerid,COLOR_GRAD, "Игрок уже оглушен");
		TogglePlayerControllable(playerid, 0);
		ApplyAnimation(playerid, "CRACK", "crckdeth2", 4.0, 1, 0, 0, 0, 0);
		Tazer[playerid] = true;
		SetTimerEx("TazerOff", 10000, 0, "d", playerid);
		format(query, sizeof(query), "%s %s оглушил Вас на 15 секунд", RankName(issuerid), Player[issuerid][pName]);
		SCM(playerid, 0x0099FFAA, query);
		format(query, sizeof(query), "Вы оглушили %s на 15 секунд", Player[playerid][pName]);
		SCM(issuerid, 0x0099FFAA, query);
		ApplyAnimation(playerid, "CRACK", "crckdeth2", 4.0, 1, 0, 0, 0, 0);
	}
	return 1;
}
