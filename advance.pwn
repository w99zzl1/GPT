f_public MuteTime()
{
	new hourass,mintaser,serev;
	gettime(hourass,mintaser,serev);
	if(capturestime > 0) capturestime -= 1;
	if(affect_time > 0) affect_time -= 1;
	if(mintaser == 0 && serev == 1)
	{
		SetWorldTime(hourass);
	}
	foreach(new i: Player)
	{
	    if(admin_combo[i][pTimer] > 0)
	    {
	        admin_combo[i][pTimer] --;
